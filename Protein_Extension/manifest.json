{
  "name": "Protein Calculator",
  "version": "1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": ["storage", "notifications"]
}
